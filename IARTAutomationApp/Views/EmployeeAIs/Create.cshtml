        btnRemove.attr("onclick", "Remove(this, tblConf);");
        btnRemove.val("Remove");
        cell.append(btnRemove);

        $('#ConferenceAttendedName').val('');
        $('#ConferenceAttendedTitle').val('');
        $('#ConferenceAttendedDate').val('');

    });


    function Remove(button,tableName) {
        //Determine the reference of the Row using the Button.
        var row = $(button).closest("TR");
        var name = $("TD", row).eq(0).html();
        if (confirm("Do you want to delete: " + name)) {
            //Get the reference of the Table.
            var table = $(tableName)[0];

            //Delete the Table row using it's Index.
            table.deleteRow(row[0].rowIndex);
        }
    };


    $("body").on("click", "#btnSave", function () {
        
        
        //Loop through the Table rows and build a JSON array.
        var ObjAssociation = new Array();
        var ObjConferenece = new Array();


        $("#tblAsso TBODY TR").each(function () {
            var row = $(this);
            var association = {};
            association.EmployeeCode = $("#EmployeeCode").val();
            association.Title = row.find("TD").eq(0).html();
            association.IDNumber = row.find("TD").eq(1).html();
            association.AttendedDate = row.find("TD").eq(2).html();
            association.CreatedDate=  row.find("TD").eq(2).html();
            association.IsDeleted  =false;
            ObjAssociation.push(association);
        });

        $("#tblConf TBODY TR").each(function () {
            var row = $(this);
            var conference = {};
            conference.EmployeeCode = $("#EmployeeCode").val();
            conference.Name = row.find("TD").eq(0).html();
            conference.Title = row.find("TD").eq(1).html();
            conference.AttendedDate = row.find("TD").eq(2).html();
            conference.CreatedDate=  row.find("TD").eq(2).html();
            conference.IsDeleted  =false;
            ObjConferenece.push(conference);
        });
        var objAcademic=[];
         var obj={};
        obj.EmployeeCode=$('#EmployeeCode').val();
        obj.InstitutionAttended1 = $('#InstitutionAttended1').val();
        obj.InstitutionAttended2 = $('#InstitutionAttended2').val();
        obj.InstitutionAttended3 = $('#InstitutionAttended3').val();
        obj.InstitutionAttended1 = $('#InstitutionAttended1').val();
        obj.Qualification1 = $('#Qualification1 :selected').text();
        obj.Qualification2 = $('#Qualification2 :selected').text();
        obj.Qualification3 =$('#Qualification3 :selected').text();
        obj.YearOfGraduation1 =$('#YearOfGraduation1 :selected').text();
        obj.YearOfGraduation2 =$('#YearOfGraduation2 :selected').text();
        obj.YearOfGraduation3 = $('#YearOfGraduation3 :selected').text();
 
       
        //var model = {
 
        //    "ObjAssociation": ObjAssociation,
 
        //    "ObjConferenece": ObjConferenece 
        //}  
         
        //Send the JSON array to Controller using AJAX.
        $.ajax({
            type: "POST",
            url: "/EmployeeAIs/InsertCustomers",
            //data:  model,
            data: JSON.stringify({obj:obj, ObjAssociation: ObjAssociation, ObjConferenece: ObjConferenece}), //stringify the whole data again

             contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (r) {
                
                if(r>0)
                    alert("Employee Academic Record is saved Successfully");
                else
                    alert("Please Enter Correct Data");
                window.location = '/EmployeeAIs/Create';
            }
        });
    });
</script>
